{
    // WARNING: The args overwrite the hydra config from ../config/config.yaml.
    "version": "0.2.0",
    "configurations": [
        {
            "name": "train",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/train.py",
            "console": "integratedTerminal",
            "args": [
                "run_type=debug",
                "host=gin",
                "run_type.log_to_wandb=false",
            ],
            "justMyCode": false,
            "env": {
                "CUDA_LAUNCH_BLOCKING": "1",
                "TORCH_USE_CUDA_DSA": "1",
            }
        },
        {
            "name": "predict",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/predict.py",
            "console": "integratedTerminal",
            "args": [
                // "run_type=debug",
                "run_type=release",
                "host=gin",
                "run_type.log_to_wandb=false",
                "checkpoint=best_val_iou",
                // "experiment.country=Switzerland",
                "eval.split=val",
                "experiment=hisup_fusion",
                "experiment.name=early_fusion_vit_cnn_bs2x16_mnv64"
                // "dataset.size=224015"
            ],
            "justMyCode": false,
            "env": {
                "CUDA_LAUNCH_BLOCKING": "1",
                "TORCH_USE_CUDA_DSA": "1",
            }
        },
        {
            "name": "predict_demo",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/predict_demo.py",
            "console": "integratedTerminal",
            "args": [
                "run_type=debug",
                "host=gin",
                "checkpoint=best_val_iou",
                "experiment=ffl_fusion",
                "+image_file=demo_data/image0_CH_val.tif",
                "+lidar_file=demo_data/lidar0_CH_val.copc.laz",
            ],
            "justMyCode": false
        },
        {
            "name": "lidar_density_ablation",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/lidar_density_ablation.py",
            "console": "integratedTerminal",
            "args": [
                "run_type=debug",
                "host=gin",
                "run_type.log_to_wandb=false",
            ],
            "justMyCode": false
        },
        {
            "name": "modality_ablation",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/modality_ablation.py",
            "console": "integratedTerminal",
            "args": [
                "run_type=debug",
                "host=gin",
                "run_type.log_to_wandb=false",
            ],
            "justMyCode": false
        },
        {
            "name": "all_countries",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/all_countries.py",
            "console": "integratedTerminal",
            "args": [
                "run_type=debug",
                "host=gin",
                "run_type.log_to_wandb=false",
                "experiment.country=all"
            ],
            "justMyCode": false
        },
        {
            "name": "evaluate",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/evaluate.py",
            "console": "integratedTerminal",
            "args": [
                "run_type=release",
                "host=gin",
                "run_type.log_to_wandb=false",
                "eval.split=test",
                "checkpoint=best_val_iou",
                "experiment=lidar_density_ablation512"
            ],
            "justMyCode": false
        },
        {
            "name": "results_to_latex",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/results_to_latex.py",
            "console": "integratedTerminal",
            "args": [
                "run_type=release",
                "host=gin",
                "run_type.log_to_wandb=false",
                "host.multi_gpu=false",
            ],
            "justMyCode": false
        },
        {
            "name": "preprocess_ffl",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/data_preprocess/ffl/preprocess_ffl.py",
            "console": "integratedTerminal",
            "args": [
                // "run_type=release",
                "run_type=debug",
                "host=gin",
                "run_type.log_to_wandb=false",
                // "experiment.country=NY"
                // "experiment.encoder=pointpillars_vit" // setting an encoder here, simply to load the 224x224 dataset version
            ],
            "justMyCode": false
        },        {
            "name": "merge_coco_annotations",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/data_preprocess/merge_coco_annotations.py",
            "console": "integratedTerminal",
            "args": [
                // "run_type=release",
                "run_type=debug",
                "host=gin",
                "run_type.log_to_wandb=false",
                "experiment.country=Switzerland"
                // "experiment.encoder=pointpillars_vit" // setting an encoder here, simply to load the 224x224 dataset version
            ],
            "justMyCode": false
        },
        {
            "name": "current_file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "run_type=debug",
            ],
            "justMyCode": false
        }
    ]
}